{
  "name": "Recovery Protocols",
  "version": "1.0",
  "protocols": [
    {
      "severity": "low",
      "actions": ["log_event", "resume_next_action"]
    },
    {
      "severity": "medium",
      "actions": [
        "pause_non_critical_tasks",
        "trigger_weekly_review",
        "apply_scope_reduction"
      ]
    },
    {
      "severity": "high",
      "actions": [
        "halt_strategy_execution",
        "trigger_full_review",
        "activate_recovery_window"
      ]
    }
  ]
}
